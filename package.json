{
  "name": "questioner",
  "version": "1.0.0",
  "description": "This application helps the meetup organizer prioritize questions to be answered. It allows users to create meetups, delete meetups and other functionalities",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:server && node dist/server.js",
    "start:dev": "nodemon --exec babel-node server/server.js",
    "build:server": "babel ./server -d dist",
    "build-babel": "babel -d ./build ./server -s",
    "test": " mocha --require babel-core/register ./server/test/*.test.js --exit",
    "test-watch": "nodemon --exec 'npm test' ",
    "dev-start": "babel-watch server/server.js",
    "heroku-postbuild": "npm run build:server",
    "compile": "babel --presets es2015,stage-0 -d lib/ server/",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidokonji/Questioner.git"
  },
  "author": "Okonji Chukwunonso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidokonji/Questioner/issues"
  },
  "homepage": "https://github.com/davidokonji/Questioner#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.4",
    "uuid": "^3.3.2"
  }
}
